{
  "name": "setup-liquibase",
  "version": "2.0.0",
  "description": "Set up your GitHub Actions workflow with a specific version of Liquibase",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "jest --maxWorkers=1 --forceExit",
    "test:ci": "jest --ci --maxWorkers=1 --forceExit --no-coverage",
    "test:memory": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" jest --maxWorkers=1 --forceExit",
    "test:debug": "jest --maxWorkers=1 --detectOpenHandles --forceExit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "package": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liquibase/setup-liquibase.git"
  },
  "keywords": [
    "actions",
    "liquibase",
    "setup",
    "database",
    "migration"
  ],
  "author": "Liquibase",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/http-client": "^3.0.0",
    "@actions/io": "^2.0.0",
    "@actions/tool-cache": "^2.0.1",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vercel/ncc": "^0.38.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-jest": "^29.2.1",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "inflight": "npm:@isaacs/inflight@^1.0.6",
    "glob": "^10.0.0",
    "rimraf": "^5.0.0"
  }
}
